{
    "name": "swoft/redis",
    "type": "library",
    "keywords": [
        "php",
        "swoole",
        "swoft",
        "redis"
    ],
    "description": "Redis Component for Swoft",
    "license": "Apache-2.0",
    "require": {
        "psr/simple-cache": "^1.0.0",
        "swoft/framework": "^1.0"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Swoft\\Redis\\": "src/"
        },
        "files": [
            "src/Helper/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\Redis\\": "test/Cases",
            "SwoftTest\\Redis\\Testing\\": "test/Testing"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    ],
    "require-dev": {
        "swoft/service-governance": "^1.0",
        "swoft/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml"
    }
}
