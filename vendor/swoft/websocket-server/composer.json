{
  "name": "swoft/websocket-server",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "websocket-server",
    "websocket"
  ],
  "description": "websocket server for swoft",
  "license": "Apache-2.0",
  "require": {
    "swoft/http-server": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\WebSocket\\Server\\": "src/"
    },
    "files": [
      "src/Helper/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\WebSocket\\Server\\": "test/Cases"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  }
}
