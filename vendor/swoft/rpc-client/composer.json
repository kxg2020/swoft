{
    "name": "swoft/rpc-client",
    "type": "library",
    "keywords": [
        "php",
        "swoole",
        "swoft",
        "rpc"
    ],
    "description": "microservice framework base on swoole",
    "license": "Apache-2.0",
    "require": {
        "swoft/framework": "^1.0"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^5.7",
        "swoft/rpc": "^1.0",
        "swoft/rpc-server": "^1.0",
        "swoft/task": "^1.0",
        "swoft/process": "^1.0",
        "swoft/service-governance": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Swoft\\Rpc\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\Rpc\\Client\\": "test/Cases/",
            "SwoftTest\\Rpc\\Testing\\": "test/Testing/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml"
    }
}
